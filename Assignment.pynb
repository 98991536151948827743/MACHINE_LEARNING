import random


subjects = ["Physics", "Chemistry", "Mathematics"]

data = {}

for subject in subjects:
    marks = []  # empty list for this subject
    for i in range(20):  # 20 students
        marks.append(random.randint(0, 100))  # random marks 0â€“100
    data[subject] = marks

# Print results
print("Marks of 20 students:\n")
for subject, marks in data.items():
    print(subject, ":", marks)
